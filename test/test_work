base64 /dev/urandom | head -c 50 | grep a
ls *
ls [a]*
ls [!a]*
ls [!a-e]*
ls [a-e]*
ls {a..z}
ls {a..z}{1..10}
ls {a,{b,c}{1..10}}
ls {a,{b,c}{1..10}}{a..b}
ls {a,{b,c}{1..10}}{a..b}

||
&&
2>1
2>&1
ls || ls -l
echo "it works " | cat -e
ls && ;
kls ; 123 ; ls ; ls <
